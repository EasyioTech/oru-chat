{
    "token_hmac_secret_key": "env:JWT_SECRET",
    "api_key": "env:CENTRIFUGO_API_KEY",
    "admin_password": "env:CENTRIFUGO_ADMIN_PASSWORD",
    "admin_secret": "env:CENTRIFUGO_ADMIN_SECRET",
    "allowed_origins": [
        "*"
    ],
    "engine": "redis",
    "redis_address": "redis://redis:6379",
    "namespaces": [
        {
            "name": "channel",
            "presence": true,
            "join_leave": true,
            "history_size": 100,
            "history_ttl": "300s",
            "allow_subscribe_for_client": true
        },
        {
            "name": "user",
            "presence": false,
            "join_leave": false,
            "history_size": 0,
            "allow_subscribe_for_client": true
        },
        {
            "name": "workspace",
            "presence": true,
            "join_leave": true,
            "history_size": 50,
            "history_ttl": "180s",
            "allow_subscribe_for_client": true
        }
    ],
    "log_level": "info",
    "health": true,
    "admin": true
}